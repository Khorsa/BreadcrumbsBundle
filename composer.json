{
  "name": "flexycms/breadcrumbs-bundle",
  "version": "0.1.1",
  "type": "symfony-bundle",
  "description": "Bundle for breadcrumbs control",
  "license": "proprietary",
  "require": {
    "php": "^7.3"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "flexycms\\BreadcrumbsBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flexycms\\BreadcrumbsBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  }
}